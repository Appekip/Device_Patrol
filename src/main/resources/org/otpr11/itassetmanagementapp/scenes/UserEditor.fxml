<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
  minHeight="-Infinity" minWidth="-Infinity" prefHeight="356.0" prefWidth="265.0"
  xmlns="http://javafx.com/javafx/17"
  fx:controller="org.otpr11.itassetmanagementapp.ui.controllers.UserEditorController">
  <children>
    <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0">
      <VBox.margin>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
      </VBox.margin>
      <children>
        <Button alignment="TOP_LEFT" maxHeight="25.0" maxWidth="52.0" minHeight="25.0"
          minWidth="52.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="52.0"
          text="Cancel" fx:id="cancelButton">
          <HBox.margin>
            <Insets right="10.0"/>
          </HBox.margin>
        </Button>
        <Button maxHeight="25.0" maxWidth="52.0" minHeight="25.0" minWidth="52.0" mnemonicParsing="false"
          prefHeight="25.0" prefWidth="52.0" text="OK" fx:id="okButton">
          <HBox.margin>
            <Insets left="10.0"/>
          </HBox.margin>
        </Button>
      </children>
      <padding>
        <Insets bottom="30.0" left="30.0" right="30.0" top="10.0"/>
      </padding>
    </HBox>
    <Label text="First Name" fx:id="firstName">
      <padding>
        <Insets left="20.0" right="20.0" top="10.0"/>
      </padding>
    </Label>
    <Label text="Last Name" fx:id="lastName">
      <padding>
        <Insets left="20.0" right="20.0" top="10.0"/>
      </padding>
    </Label>
    <Label text="Phone number" fx:id="phoneNumber">
      <padding>
        <Insets left="20.0" right="20.0" top="10.0"/>
      </padding>
    </Label>
    <Label text="Email" fx:id="email">
      <padding>
        <Insets left="20.0" right="20.0" top="10.0"/>
      </padding>
    </Label>
    <Label text="Employee Id" fx:id="id">
      <padding>
        <Insets left="20.0" right="20.0" top="10.0"/>
      </padding>
    </Label>
    <TextField fx:id="firstNameField"/>
    <TextField fx:id="lastNameField"/>
    <TextField fx:id="phoneNumberField"/>
    <TextField fx:id="emailField"/>
    <TextField fx:id="employeeIdField"/>
  </children>
  <padding>
    <Insets bottom="20.0" left="40.0" right="40.0" top="20.0"/>
  </padding>
</VBox>

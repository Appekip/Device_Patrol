<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.otpr11.itassetmanagementapp.ui.controllers.ManagementViewController">

  <children>
    <BorderPane fx:id="ManagementViewPane" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <center>
        <TableView fx:id="managementTable" BorderPane.alignment="CENTER">
          <BorderPane.margin>
            <Insets />
          </BorderPane.margin>
          <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
          </columnResizePolicy>
          <columns>
            <TableColumn fx:id="configColumn" maxWidth="1.7976931348623157E308" minWidth="105.0" prefWidth="-1.0" text="HW Configuration" />
            <TableColumn fx:id="configActionColumn" maxWidth="-1.0" minWidth="110.0" prefWidth="-1.0" style="-fx-alignment: CENTER;" text="Action" />
            <TableColumn fx:id="osColumn" maxWidth="1.7976931348623157E308" minWidth="105.0" prefWidth="-1.0" text="Operating Systems" />
            <TableColumn fx:id="osActionColumn" maxWidth="-1.0" minWidth="110.0" prefWidth="-1.0" style="-fx-alignment: CENTER;" text="Action" />
            <TableColumn fx:id="userColumn" maxWidth="-1.0" minWidth="110.0" prefWidth="-1.0" style="-fx-alignment: CENTER;" text="User" />
            <TableColumn fx:id="userActionColumn" maxWidth="-1.0" minWidth="110.0" prefWidth="-1.0" style="-fx-alignment: CENTER;" text="Action" />
            <TableColumn fx:id="locationColumn" maxWidth="-1.0" minWidth="110.0" prefWidth="-1.0" style="-fx-alignment: CENTER;" text="Location" />
            <TableColumn fx:id="locationActionColumn" maxWidth="-1.0" minWidth="110.0" prefWidth="-1.0" style="-fx-alignment: CENTER;" text="Action" />
          </columns>
        </TableView>
      </center>
    </BorderPane>
  </children>

</AnchorPane>

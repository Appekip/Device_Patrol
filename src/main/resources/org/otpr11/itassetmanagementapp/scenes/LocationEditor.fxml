<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefWidth="225.0"
  xmlns="http://javafx.com/javafx/16"
  fx:controller="org.otpr11.itassetmanagementapp.ui.controllers.LocationEditorController">
  <children>
    <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
      AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" maxHeight="1.7976931348623157E308"
      maxWidth="1.7976931348623157E308">
      <bottom>
        <HBox BorderPane.alignment="CENTER" alignment="CENTER"
          maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
          <children>
            <Button alignment="TOP_LEFT" cancelButton="true" mnemonicParsing="false"
              text="Cancel" fx:id="cancelButton">
              <HBox.margin>
                <Insets right="10.0"/>
              </HBox.margin>
            </Button>
            <Button defaultButton="true" maxHeight="25.0" maxWidth="52.0" minHeight="25.0"
              minWidth="52.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="52.0"
              text="Save" fx:id="okButton">
              <HBox.margin>
                <Insets left="10.0"/>
              </HBox.margin>
            </Button>
          </children>
          <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
          </padding>
        </HBox>
      </bottom>
      <center>
        <VBox BorderPane.alignment="CENTER" alignment="CENTER">
          <BorderPane.margin>
            <Insets/>
          </BorderPane.margin>
          <children>
            <Label text="ID" fx:id="idText">
              <padding>
                <Insets left="20.0" right="20.0" top="10.0"/>
              </padding>
            </Label>
            <Label text="Nickname" fx:id="nickText">
              <padding>
                <Insets left="20.0" right="20.0" top="10.0"/>
              </padding>
            </Label>
            <Label text="Address" fx:id="addressText">
              <padding>
                <Insets left="20.0" right="20.0" top="10.0"/>
              </padding>
            </Label>
            <Label text="ZIP code" fx:id="zipText">
              <padding>
                <Insets left="20.0" right="20.0" top="10.0"/>
              </padding>
            </Label>
            <TextField fx:id="idField"/>
            <TextField fx:id="addressField"/>
            <TextField fx:id="nicknameField"/>
            <TextField fx:id="zipCodeField"/>
          </children>
          <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
          </padding>
        </VBox>
      </center>
      <top>
        <Text BorderPane.alignment="CENTER" strokeType="OUTSIDE" strokeWidth="0.0" text="Location">
          <BorderPane.margin>
            <Insets top="10.0"/>
          </BorderPane.margin>
          <font>
            <Font size="16.0"/>
          </font>
        </Text>
      </top>
    </BorderPane>
  </children>
</AnchorPane>
